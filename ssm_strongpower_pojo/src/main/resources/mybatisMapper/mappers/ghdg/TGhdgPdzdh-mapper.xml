    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        T_PDZDH_ID, T_PDZDH_TYPE, T_PDZDH_DYDJ, T_PDZDH_ZDHFGL, T_PDZDH_PDZDYYBL, T_PDZDH_PDZDEYBL, T_PDZDH_PDZDSYBL
    </sql>
    <select id="selectPage" resultMap="SelResultMap" parameterType="com.kylin.electricassistsys.pojo.ghdg.TGhdgPdzdhSel">
        SELECT
                T_GHDG_PDZDH.T_PDZDH_ID,
                T_GHDG_PDZDH.T_PDZDH_TYPE,
                T_JCSJ_FQ_GQLX.T_GQLX_NAME,
                T_GHDG_PDZDH.T_PDZDH_DYDJ,
                T_JCSJ_CS_DYCS.T_DYCS_NAME,
                T_GHDG_PDZDH.T_PDZDH_ZDHFGL,
                T_GHDG_PDZDH.T_PDZDH_PDZDYYBL,
                T_GHDG_PDZDH.T_PDZDH_PDZDEYBL,
                T_GHDG_PDZDH.T_PDZDH_PDZDSYBL
        FROM
                T_GHDG_PDZDH
                LEFT JOIN T_JCSJ_FQ_GQLX ON (
                    T_JCSJ_FQ_GQLX.T_GQLX_ID = T_GHDG_PDZDH.T_PDZDH_TYPE
                )
                LEFT JOIN T_JCSJ_CS_DYCS ON (
                    T_JCSJ_CS_DYCS.T_DYCS_ID = T_GHDG_PDZDH.T_PDZDH_DYDJ
                )
        WHERE
                1=1  ${ew.sqlSegment}
