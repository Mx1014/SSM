    <!-- 自定义查询映射结果 -->
    <resultMap id="SelResultMap" type="com.kylin.electricassistsys.pojo.ghdg.TGhdgPdzdhSel">
        <id column="T_PDZDH_ID" property="tPdzdhId"/>
        <result column="T_PDZDH_TYPE" property="tPdzdhType"/>
        <result column="T_PDZDH_DYDJ" property="tPdzdhDydj"/>
